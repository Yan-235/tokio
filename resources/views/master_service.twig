                                            <input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}">
                                        </div>
                                        <tbody>
                                        <tr>
                                            <input type="number" name="product_id" value="{{ product_feedback.product_id }}" hidden>
                                            <td class="text-center">{{ product_feedback.name }}</td>
                                            <td>
                                                <input type="number" max="100" min="0.01" step="0.01"
                                                       value="{{ product_feedback.feedback }}" name="feedback">
                                            </td>
                                            <td>
                                                <div class="input-field text-center">
                                                    <button type="submit" class="waves-effect waves-light btn" href="#">Изменить</button>
                                                </div>
                                            </td>
                                        </tr>
                                        </tbody>
                                    </form>
                                {% endfor %}
                            </table>

                        </div>
                    </div>
                </div>
