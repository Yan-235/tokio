                    <div class="col s6">
                        <button class="waves-effect waves-light btn grey" onclick="show_goods()">Товары</button>
                    </div>
                </section>
                <section id="show_goods">
                    <div class="col s6">
                        <button class="waves-effect waves-light btn grey" onclick="show_services()">Услуги</button>
