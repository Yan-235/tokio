                        <button class="waves-effect waves-light btn" onclick="show_goods()">Товары</button>
                    </div>
                </section>
            {% else %}
                <section id="show_services" hidden>
                    <div class="col s6">
                        <button class="waves-effect waves-light btn" onclick="show_services()">Услуги</button>
